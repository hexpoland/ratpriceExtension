{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "background": {
      "pages":["index.html"],
        "scripts": ["background.js","jquery.min.js","index.js"],
        "persistent": false
      },
      "permissions":[
        //   "browser",
        "background",
          "activeTab",
          "storage",
          "tabs",
          "contextMenus"
      ],

     
      "page_action": {
        "default_popup": "index.html"
      },
      "content_scripts": [
        {
          "matches": [
            "https://portal.rational-online.com/external/*"
          ],
          "js": ["jquery.min.js","content.js"]
        }
      ],
    "manifest_version": 2
  }